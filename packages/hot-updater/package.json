{
  "name": "hot-updater",
  "type": "module",
  "version": "0.21.5",
  "bin": {
    "hot-updater": "./dist/index.js"
  },
  "main": "dist/config.cjs",
  "module": "dist/config.js",
  "types": "dist/config.d.ts",
  "exports": {
    ".": {
      "node": "./dist/config.cjs",
      "import": "./dist/config.js",
      "require": "./dist/config.cjs",
      "types": "./dist/config.d.ts"
    },
    "./babel-plugin": {
      "node": "./dist/plugins/babel.cjs",
      "import": "./dist/plugins/babel.js",
      "require": "./dist/plugins/babel.cjs",
      "types": "./dist/plugins/babel.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "keywords": [
    "react-native",
    "react-native-code-push",
    "code-push",
    "eas",
    "eas-update",
    "expo",
    "expo-update",
    "self-hosted"
  ],
  "description": "React Native OTA solution for self-hosted",
  "license": "MIT",
  "repository": "https://github.com/gronxb/hot-updater",
  "author": "gronxb <gron1gh1@gmail.com> (https://github.com/gronxb)",
  "bugs": {
    "url": "https://github.com/gronxb/hot-updater/issues"
  },
  "homepage": "https://github.com/gronxb/hot-updater#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "test:type": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "catalog:",
    "@expo/fingerprint": "0.13.4",
    "@hot-updater/console": "workspace:*",
    "@hot-updater/core": "workspace:*",
    "@hot-updater/plugin-core": "workspace:*",
    "cosmiconfig": "9.0.0",
    "jiti": "2.6.1",
    "sql-formatter": "15.6.10",
    "cosmiconfig-typescript-loader": "5.0.0"
  },
  "devDependencies": {
    "semver": "^7.6.3",
    "fast-xml-parser": "^5.2.3",
    "fast-glob": "3.3.3",
    "@bacons/xcode": "1.0.0-alpha.24",
    "@commander-js/extra-typings": "^14.0.0",
    "commander": "^14.0.0",
    "@babel/core": "7.26.0",
    "@babel/generator": "7.26.9",
    "@babel/parser": "7.26.9",
    "@babel/traverse": "7.25.9",
    "@babel/types": "7.26.0",
    "@hono/node-server": "^1.13.4",
    "@hot-updater/aws": "workspace:*",
    "@hot-updater/server": "workspace:*",
    "@hot-updater/cloudflare": "workspace:*",
    "@hot-updater/firebase": "workspace:*",
    "@hot-updater/supabase": "workspace:*",
    "@hot-updater/test-utils": "workspace:*",
    "@types/babel__core": "7.20.5",
    "@types/babel__generator": "7.6.8",
    "@types/babel__traverse": "7.20.6",
    "@types/connect": "^3.4.38",
    "@types/cosmiconfig": "^6.0.0",
    "@types/node": "catalog:",
    "@types/plist": "^3.0.5",
    "@types/semver": "^7.5.8",
    "es-toolkit": "^1.32.0",
    "execa": "catalog:",
    "is-port-reachable": "^4.0.0",
    "open": "^10.1.0",
    "picocolors": "catalog:",
    "plist": "^3.1.0",
    "read-package-up": "^11.0.0",
    "uuidv7": "^1.0.2"
  },
  "peerDependencies": {
    "@hot-updater/aws": "*",
    "@hot-updater/cloudflare": "*",
    "@hot-updater/firebase": "*",
    "@hot-updater/supabase": "*"
  },
  "peerDependenciesMeta": {
    "@hot-updater/supabase": {
      "optional": true
    },
    "@hot-updater/cloudflare": {
      "optional": true
    },
    "@hot-updater/aws": {
      "optional": true
    },
    "@hot-updater/firebase": {
      "optional": true
    },
    "@hot-updater/server": {
      "optional": true
    }
  }
}
