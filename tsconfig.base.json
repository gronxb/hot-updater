{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "esnext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "isolatedModules": true,
    "paths": {
      "@hot-updater/aws": [
        "./plugins/aws/src/index.ts"
      ],
      "hot-updater": [
        "./packages/hot-updater/src/index.ts"
      ]
    },
    "strict": true,
    "declaration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "pretty": true
  }
}
